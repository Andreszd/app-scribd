nts\\NavUser\\index.js",["157","158"],"import  { useState } from 'react';\r\n\r\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome'\r\nimport { faUser, faFileUpload, faBars } from '@fortawesome/free-solid-svg-icons'\r\n\r\nimport Button from '../Button'\r\nimport Search from \"../Search\";\r\nimport UserMenu from '../DropDownUser';\r\nimport {useHistory} from 'react-router-dom'\r\n\r\nimport './style.scss'\r\nexport default function NavUser({}){\r\n    const history = useHistory()\r\n\r\n    const [showMenu, setShowMenu] = useState(false)\r\n\r\n    const showDropDown = evt =>{\r\n        showMenu ? setShowMenu(false) : setShowMenu(true)\r\n    }    \r\n    return (\r\n       <nav className=\"nav-user\">\r\n           <Search/>\r\n           <Button\r\n            modifier=\"hidden-view-desktop\"\r\n            modifier={['hidden-view-desktop','padding-left']}\r\n            icon={<FontAwesomeIcon icon={faBars}/>}\r\n           />\r\n           <div className=\"nav-user__buttons\">\r\n           <Button \r\n           onClick={()=> history.push('/home/upload')}\r\n           text=\"Upload\" \r\n           modifier=\"hidden-min-width\" \r\n           icon={<FontAwesomeIcon icon={faFileUpload} className=\"icon\"/>}/>\r\n            <div className=\"profile-icon\">\r\n           <Button \r\n            onClick={showDropDown}\r\n            icon={<FontAwesomeIcon icon={faUser}/>} \r\n            />\r\n            </div>\r\n           </div>\r\n            {\r\n                showMenu ? <UserMenu/> : null \r\n            }\r\n       </nav> \r\n    )\r\n}","C:\\Users\\eva\\Desktop\\projects\\react\\app-scribd\\src\\components\\Sections\\SectionInfo.js",[],"C:\\Users\\eva\\Desktop\\projects\\react\\app-scribd\\src\\components\\Sections\\data.js",[],"C:\\Users\\eva\\Desktop\\projects\\react\\app-scribd\\src\\components\\Form\\FormLoginRegister.js",[],"C:\\Users\\eva\\Desktop\\projects\\react\\app-scribd\\src\\hooks\\useModal.js",[],"C:\\Users\\eva\\Desktop\\p